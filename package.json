{
  "name": "@boilerplate-react-vite-modular/frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "build-electron/main.js",
  "dependencies": {
    "axios": "^1.10.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-scanner": "^4.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.6.3",
    "vite": "^7.1.6",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.6.2",
    "cssnano": "^7.0.7",
    "dotenv": "^17.2.1",
    "electron": "^39.2.6",
    "eslint": "^9.31.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.0.11",
    "i18next-scanner-typescript": "^1.2.1",
    "jiti": "^2.5.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.69.5",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.12.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-inspect": "^11.3.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  },
  "scripts": {
    "dev:react": "vite --mode development",
    "dev:react:prod": "yarn run theme:generate && vite --mode production",
    "dev:electron": "electron .",
    "build": "yarn run theme:generate && vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:analyze": "yarn build && npx vite-bundle-analyzer build/stats.html",
    "preview": "vite preview --mode production",
    "preview:staging": "vite preview --mode staging",
    "preview:development": "vite preview --mode development",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:fix": "yarn run eslint:fix && yarn run stylelint:fix",
    "type-check": "tsc --noEmit",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --no-warn-ignored",
    "eslint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0 --no-warn-ignored",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint **/*.scss --fix",
    "test:vitest": "vitest run",
    "test:vitest:ui": "vitest",
    "test:unit": "vitest run --project unit",
    "test:unit:ui": "vitest --project unit",
    "i18n:scan": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:generate-types": "tsx scripts/i18n/generate-types.script.ts",
    "i18n:extract": "tsx scripts/i18n/safe-extract.script.ts",
    "theme:generate": "tsx scripts/theme/generate-theme-utilities.script.ts && yarn run eslint:fix src/renderer/shared/lib/hooks/theme-utilities",
    "generate": "tsx scripts/generators/generate.script.ts",
    "generate:component": "tsx scripts/generators/generate.script.ts component",
    "generate:module": "tsx scripts/generators/generate.script.ts module",
    "generate:service": "tsx scripts/generators/generate.script.ts service",
    "generate:hook": "tsx scripts/generators/generate.script.ts hook",
    "transpile:electron": "yarn run tsc --project tsconfig.electron.json",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "packageManager": "yarn@4.10.3"
}
